isbn = {978-1-60558-002-9},
keywords = {ambient feedback,interaction design,persuasive,serious game},
pages = {405--414},
title = {{Reflecting human behavior to motivate desirable lifestyle}},
url = {http://dl.acm.org/citation.cfm?id=1394489 http://doi.acm.org/10.1145/1394445.1394489},
year = {2008}
