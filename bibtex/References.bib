@article{Ullah2012,
abstract = {Radio Frequency IDentification (RFID) has allowed the realization of ubiquitous tracking and monitoring of physical objects wirelessly with minimum human interactions. It plays a key role in a wide range of applications including asset tracking, contactless payment, access control, transportation and logistics, and other industrial applications. On the other side, RFID systems face several technical challenges that need to be overcome in order to achieve their potential benefits; tags collisions and localization of tagged objects are two important challenges. Numerous anti-collision and localization protocols have been proposed to address these challenges. This paper reviews the state-of-art tags' anti-collision and localization protocols, and provides a deep insight into technical issues of these protocols. The probabilistic and deterministic anti-collision protocols are critically studied and compared in terms of different parameters. We further review distance estimation, scene analysis, and proximity localization schemes and provide useful suggestions. We also introduce a new hybrid direction that utilizes power control to spatially partition the interrogation range of a reader for more efficient anti-collision and localization. Finally, we present the applications of RFID systems in healthcare sectors.},
author = {Ullah, S and Alsalih, W and Alsehaim, a and Alsadhan, N},
doi = {10.1007/s10916-012-9876-5},
file = {:Users/BTB/Library/Application Support/Mendeley Desktop/Downloaded/Ullah et al. - 2012 - A review of tags anti-collision and localization protocols in RFID networks.pdf:pdf},
issn = {0148-5598},
journal = {Journal of medical systems},
keywords = {Algorithms,Computer Communication Networks,Computer Communication Networks: instrumentation,Equipment and Supplies, Hospital,Radio Frequency Identification Device,Radio Frequency Identification Device: methods,Signal Processing, Computer-Assisted,Software},
month = dec,
number = {6},
pages = {4037--50},
pmid = {22945856},
title = {{A review of tags anti-collision and localization protocols in RFID networks.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22945856},
volume = {36},
year = {2012}
}
