@incollection{Pintrich2000,
abstract = {Self-regulated learning (SRL) concerns the application of general models of regulation and self-regulation of cognition, motivation/affect, behavior, and context to issues of learning, in particular, academic learning that takes place in school or classroom contexts. This chapter first discusses some of the common features of a number of models of SRL to provide an overview and general framework for theory and research in SRL. A second purpose of this chapter involves a discussion of motivational constructs, specifically goal orientation, may be related to processes of SRL. To accomplish these two general aims, a framework for SRL is discussed first, followed by a summary of different goal orientations and how they may be linked to the different components of SRL. As part of this discussion, a taxonomy of goal orientations is presented. It is suggested that an approach–mastery goal orientation is generally adaptive for cognition, motivation, learning, and performance in an academic context.},
author = {Pintrich, Paul R.},
booktitle = {Handbook of Self-Regulation},
doi = {10.1016/B978-012109890-2/50043-3},
isbn = {9780121098902},
issn = {0121098907},
pages = {451--502},
title = {{The Role of Goal Orientation in Self-Regulated Learning}},
url = {http://www.sciencedirect.com/science/article/pii/B9780121098902500433},
year = {2000}
}
