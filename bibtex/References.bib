@article{Emmanouilidis2013,
abstract = {Portable devices are increasingly employed in a wide range of mobile guidance applications. Typical examples are guides in urban areas, museum guides, and exhibition space aids. The demand is for the delivery of context-specific services, wherein the context is typically identified by a combination of data related to location, time, user profile, device profile, network conditions and usage scenario. A context-aware mobile guide is intended to provide guidance services adjusted to the context of the received request. The adjustment may refer to tailoring the user interface to the perceived context, as well as delivering the right type of information to the right person at the right time and the right location. It may also refer to intermediary adaptation, as in the case of mobile multimedia transmission. This paper offers a taxonomy of mobile guides considering multiple criteria. The taxonomy considers several aspects of the mobile applications space, including context awareness, client architectures, mobile user interfaces, as well as offered functionalities, highlighting functional, architectural, technological, and implementation issues. Existing implementations are classified accordingly and a discussion of research issues and emerging trends is offered. ?? 2012 Elsevier Ltd.},
author = {Emmanouilidis, Christos and Koutsiamanis, Remous-Aris Aris and Tasidou, Aimilia},
doi = {10.1016/j.jnca.2012.04.007},
file = {:Users/BTB/Library/Application Support/Mendeley Desktop/Downloaded/Emmanouilidis, Koutsiamanis, Tasidou - 2013 - Mobile guides Taxonomy of architectures, context awareness, technologies and applications.pdf:pdf;:Users/BTB/Documents/Study/PhD/Articles/Journal papers/eLearningEuropa/References/Pending/Mobile\_Guides\_Taxonomy\_of\_Architectures\_ContextAwareness\_Technologies\_and\_Applications.pdf:pdf},
isbn = {3025410787},
issn = {10848045},
journal = {Journal of Network and Computer Applications},
keywords = {Context aware services,Mobile computing,Mobile guides,Urban computing,context aware services},
month = jan,
number = {1},
pages = {103--125},
publisher = {Elsevier},
title = {{Mobile guides: Taxonomy of architectures, context awareness, technologies and applications}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S1084804512001002},
